<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sitandecider">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/dinner_plate"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/dinner_plate_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SitanDecider">
        <activity android:name=".Help_Activity"></activity>
        <activity android:name=".AddNewActivity" />
        <activity
            android:name=".ui.login.AddNewActivity"
            android:label="@string/title_activity_add_new" />
        <activity android:name=".SavedDinner" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>